{
  "source": {
    "include": [ "./lib" ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "hardwrap": true
  },
  "opts": {
    "template": "node_modules/minami",
    "recurse": true,
    "destination": "./docs/docs",
    "readme": "README.md"
  },
  "templates": {
    "sort": false
  }
}